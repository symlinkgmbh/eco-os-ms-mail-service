{
  "name": "eco-os-ms-mail-service",
  "version": "0.0.1",
  "description": "Microservice for mail management.",
  "main": "src/server.ts",
  "repository": "https://gitlab.com/symlinkde",
  "license": "Apache-2.0",
  "scripts": {
    "clean": "yarn run rimraf lib",
    "docker:build": "sh ./buildImage.sh",
    "docker:run": "sh ./runImage.sh",
    "start:dev": "ts-node src/server.ts",
    "lint:fix": "yarn run tslint --fix --project .",
    "test:unit": "yarn run jest --forceExit --json --outputFile=test_unit.json --passWithNoTests --detectOpenHandles ./__tests__",
    "test:integration:dev": "yarn run jest --json --outputFile=test_integration.json --passWithNoTests --detectOpenHandles ./__integration__/dev",
    "test:integration:enterprise": "yarn run jest --json --outputFile=test_integration.json --passWithNoTests --detectOpenHandles ./__integration__/enterprise",
    "test:integration:paranoid": "yarn run jest --json --outputFile=test_integration.json --passWithNoTests --detectOpenHandles ./__integration__/paranoid",
    "test:integration:noncommercial": "yarn run jest --json --outputFile=test_integration.json --passWithNoTests --detectOpenHandles ./__integration__/non_commercial",
    "build": "yarn clean && yarn tsc && yarn gulp compile:hard && yarn gulp inject:license && yarn gulp compile:templates"
  },
  "devDependencies": {
    "@types/config": "^0.0.34",
    "@types/express": "^4.17.0",
    "@types/fs-extra": "^8.0.0",
    "@types/jest": "^24.0.15",
    "@types/mustache": "^0.8.32",
    "@types/nodemailer": "^6.2.0",
    "@types/swagger-ui-express": "^3.0.1",
    "axios": "^0.19.0",
    "google-closure-compiler": "^20190618.0.0",
    "gulp": "^4.0.2",
    "gulp-debug": "^4.0.0",
    "gulp-flatmap": "^1.0.2",
    "gulp-inject-string": "^1.1.2",
    "husky": "^2.2.0",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "rimraf": "^2.6.3",
    "sonarqube-scanner": "^2.4.0",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "@symlinkde/eco-os-pk-api": "^0.0.*",
    "@symlinkde/eco-os-pk-core": "^0.0.*",
    "@symlinkde/eco-os-pk-log": "^0.0.*",
    "@symlinkde/eco-os-pk-models": "^0.0.*",
    "config": "^3.1.0",
    "express": "^4.17.1",
    "fs-extra": "^8.1.0",
    "inversify": "^5.0.1",
    "mustache": "^3.0.1",
    "nodemailer": "^6.2.1",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.0.7"
  },
  "company": "Symlink GmbH",
  "homepage": "https://2ndlock.com"
}
